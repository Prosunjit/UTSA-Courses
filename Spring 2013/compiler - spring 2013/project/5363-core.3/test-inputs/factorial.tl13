% finding avg of a given number of inputs.
program 
   var N as int ; 
   var I as int ; 
   var T as int ;
   var TOTAL as int ;
   
begin 
   T := readInt ; % number of input to be taken
   TOTAL := 1 ;

   I := 1 ; 
   while I <= T do 	
	  TOTAL := TOTAL * I ;
      I := I + 1 ; 
   end ; 
   
  
	 writeInt TOTAL ;

end 