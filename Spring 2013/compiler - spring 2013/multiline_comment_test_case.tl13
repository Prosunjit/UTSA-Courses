
%-    
MULTILINE COMMENT STARTS WITH "%-" AND ENDS WITH "-%"
-%
	%-  MULTILINE AND COMPLEX %- -% %-
	COMMENT -% 
	% single line comment within multiline comment
	-%
program % SINGLE LINE COMMENT
  var N as int ; 
  var SQRT as int ; 
begin 
  N:=readInt; 
  SQRT:=0; 

  while SQRT*SQRT<=N do % single line comment
    SQRT:=SQRT+1; 

  end; 

  SQRT:=SQRT-1 ; 

  writeInt SQRT ; 

end